{
  "manifest_version": 3,
  "name": "4Nums Bot",
  "version": "1",
  "content_scripts": [{
    "matches": ["https://www.4nums.com/*"],
    "js": ["content.js"],
    "run_at": "document_start"
  }],

  "web_accessible_resources": [{
    "resources": ["page-poller.js"],
    "matches": ["https://www.4nums.com/*"]
  }]
}
